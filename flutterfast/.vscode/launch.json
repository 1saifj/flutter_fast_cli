    "version": "0.2.0",
    "configurations": [
        {
            "name": "firebase",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=PAAS=firebase",
                "--dart-define-from-file=assets/internal-config.json",
                "--web-port=5000"
            ]
        },
        {
            "name": "supabase",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=PAAS=supabase",
                "--dart-define-from-file=assets/internal-config.json",
                "--web-port=5000"
            ]
        },
        {
            "name": "appwrite",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=PAAS=appwrite",
                "--dart-define-from-file=assets/internal-config.json",
                "--web-port=5000"
            ]
        },
        {
            "name": "pocketbase",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=PAAS=pocketbase",
                "--dart-define-from-file=assets/internal-config.json",
                "--web-port=5000"
            ]
        },
        {
            "name": "flutterfast (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flutterfast (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "iOS Beta Fastlane",
            "request": "launch",
            "type": "node-terminal",
            "command": "bash ${workspaceFolder}/scripts/ios_beta_fastlane.sh"
        }
    ]
}